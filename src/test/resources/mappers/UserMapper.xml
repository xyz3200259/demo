<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.huafi.demo.entity.User">
    <select id="findUseMapper" resultType="com.huafi.demo.entity.User" lang="XML">
      SELECT ID AS id, FIRST_NAME AS firstname, LASTNAME AS lastname FROM DS_USER WHERE LASTNAME = #{lastname}
    </select>
</mapper>